{
  "name": "MovieMax",
  "version": "0.0.1",
  "versionCode": 1,
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios:dev": "react-native run-ios --scheme MovieMaxDev",
    "ios:clean": "cd ios && xcodebuild clean",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform=ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "android:dev": "react-native run-android --mode=developmentdebug --appIdSuffix=development",
    "android:dev-release": "react-native run-android --mode=developmentrelease --appIdSuffix=development",
    "android:prod": "react-native run-android --mode=productiondebug",
    "android:prod-release": "react-native run-android --mode=productionrelease",
    "build:dev": "cd android && ./gradlew assembleDevelopmentRelease",
    "build:prod": "cd android && ./gradlew assembleProductionRelease",
    "bundle:prod": "cd android && ./gradlew bundleProductionRelease",
    "android:clean": "cd android && ./gradlew clean"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@tanstack/react-query": "^5.17.1",
    "axios": "^1.6.3",
    "react": "18.2.0",
    "react-native": "0.73.1",
    "react-native-config": "^1.5.1",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-svg": "^14.1.0",
    "react-native-svg-transformer": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "^0.73.18",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@react-native/typescript-config": "^0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
